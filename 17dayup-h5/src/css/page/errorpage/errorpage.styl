.has-error
    width 198px
    padding-top 89px
    margin 0 auto
        
.icon-error
    width 100%
    height 214px
    background-repeat no-repeat
    background-size 198px 214px

.icon-error.error-500
    background-image url(/static/img/icon/500.png)

.icon-error.error-404
    background-image url(/static/img/icon/404.png)    

.error-btn
    margin 40px auto 0
    width 75px

.go-index
    font-size 12px
    text-align center
    display block
    color #268fff
    width 75px
    height 25px
    line-height 25px
    border 1px solid #268fff
    border-radius 5px
 
   
